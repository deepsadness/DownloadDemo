<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context="com.cry.downloaddemo.MainActivity">

    <Button
        android:id="@+id/btn_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start_download" />

    <Button
        android:id="@+id/test_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test_header" />

    <Button
        android:id="@+id/btn_c_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_c_start" />

    <Button
        android:id="@+id/btn_write"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_write" />

    <Button
        android:id="@+id/btn_copy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_copy" />

</LinearLayout>
